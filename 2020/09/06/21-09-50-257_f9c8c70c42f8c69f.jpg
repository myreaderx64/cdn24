<script>
    $(document).ready(function () {
        $('button.clear-fields').click(function (e) {
            e.preventDefault();
            $('div#search-form-mini input:not([type=checkbox]):not([type=hidden])').val('');
            $('div#search-form-mini input[type=checkbox]:not([type=hidden])').prop('checked', false);
            $('#search-form-mini button[type=submit]').prop('disabled', true);
        });
        $('[data-filter-header]').click(function (e) {
            $('.Search').toggleClass('is-open');
            if ($('.Search').hasClass('is-open')) {
                $("#filters-button").html('OCULTAR FILTROS');
                if ($(window).width() > 767) {
                    var heightCollapse = 0;
                    $('.Search-filter--content').children().each(function (i, e) {
                        heightCollapse += $(e).outerHeight();
                    })
                    $('.Search-filter--content').animate({
                        height: heightCollapse
                    })
                }
            } else {
                $("#filters-button").html('FILTRAR');
                if ($(window).width() > 767) {
                    $('.Search-filter--content').animate({
                        height: 0
                    })
                }
            }
        });
        $(document).on('click', '.Search-filter--header button', function () {
            $(this).closest('.Search').toggleClass('is-open');
            if ($('.Search').hasClass('is-open')) {
                if ($(window).width() > 767) {
                    var heightCollapse = 0;
                    $('.Search-filter--content').children().each(function (i, e) {
                        heightCollapse += $(e).outerHeight();
                    })
                    $('.Search-filter--content').animate({
                        height: heightCollapse
                    })
                }
            } else {
                $("#filters-button").html('FILTRAR');
                if ($(window).width() > 767) {
                    $('.Search-filter--content').animate({
                        height: 0
                    })
                }
            }
        })

        

        function isSearchable() {
            var anyCheckMarked = false;
            var anyTextFilled = false;
            if ($('.Search-input input[type=checkbox]:checked').length > 0) {
                anyCheckMarked = true;
            }
            if ($('.Search-input input[type=text]').filter(function () { return $.trim($(this).val()).length > 0 }).length > 0) {
                anyTextFilled = true;
            }
            return anyCheckMarked || anyTextFilled;
        }

        
        $('#search-form-mini button[type=submit]').prop('disabled', !isSearchable());

        
        $('#filt_txt_nv').keyup(function () { $('.Search-input input:not([type=submit]):not([type=hidden])').first().change(); });
        $('.Search-input input:not([type=submit]):not([type=hidden])').change(function () {
            var checkResult = isSearchable();
            $('#search-form-mini button[type=submit]').prop('disabled', !checkResult);
            if (!checkResult) {
                $('#filt_txt_nv').addClass('input-validation-error');
                $('#search_validation_message').show();
            } else {
                $('#filt_txt_nv').removeClass('input-validation-error');
                $('#search_validation_message').hide();
            }
        });
    });
</script>

    
